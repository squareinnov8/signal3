{
  "name": "signal3",
  "version": "0.0.1",
  "private": true,
  "description": "Equifax Design System - Signal3",
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,mdx}\"",
    "test": "turbo test",
    "clean": "turbo clean && rm -rf node_modules",
    "figma:pull": "node scripts/figma-pull.mjs",
    "figma:sync": "pnpm figma:pull && pnpm tokens:build",
    "tokens:build": "node scripts/tokens-build.mjs",
    "generate:component": "node scripts/generate-component.mjs",
    "storybook": "turbo storybook",
    "docs:build": "turbo docs:build"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "eslint": "^8.56.0",
    "next": "^14.2.0",
    "prettier": "^3.1.1",
    "turbo": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "packageManager": "pnpm@8.12.0",
  "engines": {
    "node": ">=18.0.0"
  }
}
